@model TheRentersPlace.Models.Tenant

@{
    ViewBag.Title = "Messages";
}

<h2>Here are your messages</h2>
<ul>
    @foreach (var item in Model.Messages)
    {
        <li>
            <b>From: </b>@item.SenderUsername, <b>To: </b>@item.ReceiverUsername @Html.ActionLink("Reply", "TenantCreate", "Messages", new { ManagerId = item.ManagerId, ManagerUsername = item.Manager.Username }, new { @class = "btn btn-primary float-right" })<br />
            <p><b>Content: </b>@item.Content</p>
        </li>
    }
</ul>


